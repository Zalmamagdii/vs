class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
     int f=0;
     vector<int>ans;
     int size = nums.size();
    for( int i = 0; i<size-1 ; ++i){
        for(int j = i+1 ; j<size; ++j){
            if(nums[i]+nums[j]==target){
                ans.push_back(i);
                ans.push_back(j);
                f=1;
                break;
            }
        }
        if(f==1){
            break;
        }
    }
    return ans;
    }   
};
